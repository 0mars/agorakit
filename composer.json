{
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=5.5.9",
    "laravel/framework": "5.3.*",
    "watson/validating": "^2.0",
    "venturecraft/revisionable": "1.*",
    "laravelcollective/html": "5.3.*",
    "barryvdh/laravel-debugbar": "^2.1",
    "intervention/image": "^2.3",
    "intervention/imagecache": "^2.3",
    "eluceo/ical": "^0.9.0",
    "kwi/urllinker": "dev-master",
    "barryvdh/laravel-ide-helper": "^2.1",
    "rap2hpoutre/laravel-log-viewer": "^0.6.1",
    "vsch/laravel-translation-manager": "*",
    "iatstuti/laravel-cascade-soft-deletes": "1.0.*",
    "toin0u/geocoder-laravel": "^1.0",
    "suin/php-rss-writer": "^1.5",
    "cviebrock/eloquent-taggable": "^2.0",
    "laravel/socialite": "^2.0",
    "consoletvs/charts": "5.*",
    "spatie/laravel-activitylog": "1.15.5"
  },
  "require-dev": {
    "fzaninotto/faker": "~1.4",
    "mockery/mockery": "0.9.*",
    "phpunit/phpunit": "~4.0",
    "symfony/css-selector": "3.1.*",
    "symfony/dom-crawler": "3.1.*",
    "laravel/homestead": "^3.0"
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "files": [
      "app/Helpers/Functions.php",
      "workbench/draperstudio/laravel-flash/src/functions.php"
    ],
    "psr-4": {
      "App\\": "app/",
      "DraperStudio\\ServiceProvider\\": "workbench/draperstudio/laravel-service-provider/src/",
      "DraperStudio\\Flash\\": "workbench/draperstudio/laravel-flash/src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ]
  },
  "scripts": {
    "post-root-package-install": [
      "php -r \"copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "php artisan key:generate"
    ],
    "post-install-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize"
    ],
    "pre-update-cmd": [
      "php artisan clear-compiled"
    ],
    "post-update-cmd": [
      "php artisan optimize"
    ]
  },
  "config": {
    "preferred-install": "dist"
  }
}
