{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0.0",
        "askedio/laravel5-soft-cascade": "^5.5",
        "consoletvs/charts": "5.*",
        "cviebrock/eloquent-taggable": "^3.2.3",
        "eluceo/ical": "^0.13.0",
        "fideloper/proxy": "~3.3",
        "iatstuti/laravel-cascade-soft-deletes": "1.3.*",
        "intervention/image": "^2.4.1",
        "intervention/imagecache": "2.*",
        "kwi/urllinker": "dev-master",
        "laracasts/flash": "3.0.0",
        "laravel/framework": "5.5.*",
        "laravel/socialite": "^3.0",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "5.5.*",
        "laravelnews/laravel-twbs4": "^1.3",
        "potsky/laravel-localization-helpers": "2.6.*",
        "rap2hpoutre/laravel-log-viewer": "^0.11.0",
        "suin/php-rss-writer": "^1.6",
        "toin0u/geocoder-laravel": "^4.0",
        "venturecraft/revisionable": "1.*",
        "watson/autologin": "^0.5.2",
        "watson/validating": "^3.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.1",
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "laravel/browser-kit-testing": "^1.0",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "app/Helpers/Functions.php"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "classmap": [
            "tests/TestCase.php",
            "tests/BrowserKitTestCase.php"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
